{
  "name": "quickfood-delivery-app",
  "version": "1.0.0",
  "description": "Full-stack QuickFood delivery application with MongoDB backend and Next.js frontend",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "start:backend": "cd Backend && npm start",
    "start:frontend": "cd QuickFoodDelivery-main && npm start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd Backend && npm run dev",
    "dev:frontend": "cd QuickFoodDelivery-main && npm run dev",
    "prod": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "install:all": "npm install && cd Backend && npm install && cd ../QuickFoodDelivery-main && npm install",
    "build": "cd QuickFoodDelivery-main && npm run build",
    "build:all": "npm run install:all && npm run build",
    "seed": "cd Backend && npm run seed"
  },
  "keywords": [
    "food-delivery",
    "express",
    "nextjs",
    "mongodb",
    "mongoose",
    "full-stack"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
